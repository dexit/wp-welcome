{
  "name": "wp-welcome",
  "version": "1.0.5",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "dev": "wp-scripts start",
    "build": "wp-scripts build",
    "lint:css": "wp-scripts lint-style 'resources/**/*.{css,scss}'",
    "lint:css:fix": "wp-scripts lint-style 'resources/**/*.{css,scss}' --fix",
    "lint:js": "wp-scripts lint-js ./resources webpack.config.js",
    "lint:js:fix": "wp-scripts lint-js ./resources webpack.config.js --fix",
    "preinstall": "npx only-allow pnpm",
    "pot": "wpi18n makepot --domain-path=languages --exclude=vendor,deploy,node_modules",
    "textdomain": "wpi18n addtextdomain --exclude=vendor,deploy,node_modules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ernilambar/wp-welcome"
  },
  "browserslist": "> 1%, not dead",
  "devDependencies": {
    "@wordpress/scripts": "^25.3.0",
    "node-wp-i18n": "^1.2.7"
  }
}
